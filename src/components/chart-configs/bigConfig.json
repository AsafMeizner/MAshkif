          {
            "id": "endgame",
            "title": "Endgame",
            "charts": [
              {
                "id": "endgameClimbPercentByTeam",
                "chartType": "BarGraph",
                "transformations": [
                  { "type": "groupBy", "key": "teamNumber" },
                  {
                    "type": "aggregate",
                    "operations": [
                      {
                        "field": "epo",
                        "operation": "count",
                        "alias": "No",
                        "transform": "item => (item.epo==='No'?1:0)"
                      },
                      {
                        "field": "epo",
                        "operation": "count",
                        "alias": "P",
                        "transform": "item => (item.epo==='P'?1:0)"
                      },
                      {
                        "field": "epo",
                        "operation": "count",
                        "alias": "Fo",
                        "transform": "item => (item.epo==='Fo'?1:0)"
                      },
                      {
                        "field": "epo",
                        "operation": "count",
                        "alias": "Fh",
                        "transform": "item => (item.epo==='Fh'?1:0)"
                      },
                      {
                        "field": "epo",
                        "operation": "count",
                        "alias": "Os",
                        "transform": "item => (item.epo==='Os'?1:0)"
                      },
                      {
                        "field": "epo",
                        "operation": "count",
                        "alias": "Hm",
                        "transform": "item => (item.epo==='Hm'?1:0)"
                      },
                      {
                        "field": "epo",
                        "operation": "count",
                        "alias": "total",
                        "transform": "()=>1"
                      }
                    ]
                  },
                  {
                    "type": "map",
                    "func": "(team) => {\n  const keys=['No','P','Fo','Fh','Os','Hm'];\n  keys.forEach(k=>{team[k]=team.total>0 ? Math.round((team[k]/team.total)*100*100)/100:0});\n  delete team.total; return team;\n}"
                  }
                ],
                "chartSettings": {
                  "title": "Endgame Climb Percent By Team",
                  "scoringTypes": [
                    { "key": "No", "label": "No Climb", "color": "#ff4444", "stacked": true, "name": "No Climb" },
                    { "key": "P", "label": "Parked", "color": "#ff8042", "stacked": true, "name": "Parked" },
                    { "key": "Fo", "label": "Failed Climb", "color": "#ffc658", "stacked": true, "name": "Failed Climb" },
                    { "key": "Fh", "label": "Failed Harmony", "color": "#fff258", "stacked": true, "name": "Failed Harmony" },
                    { "key": "Os", "label": "Onstage", "color": "#82ca9d", "stacked": true, "name": "Onstage" },
                    { "key": "Hm", "label": "Harmony", "color": "#8884d8", "stacked": true, "name": "Harmony" }
                  ],
                  "xKey": "teamNumber",
                  "yKey": "No",
                  "xAxisLabel": "Team Number",
                  "yAxisLabel": "Percentage",
                  "yAxisMin": 0,
                  "yAxisMax": 100,
                  "sortOrder": "ascending"
                }
              },
              {
                "id": "averageEndgameTrapByTeam",
                "chartType": "BarGraph",
